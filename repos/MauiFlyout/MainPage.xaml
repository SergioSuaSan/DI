<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="MauiFlyout.MainPage">
    <FlyoutPage.Flyout>
        <ContentPage Title="Menu">
            <StackLayout>
                <Button Text="Calculadora" Clicked="OnOption1Clicked" Margin="10"/>
                <Button Text="Binario-Decimal" Clicked="OnOption2Clicked"  Margin="10"/>
                <Button Text="Página 3" Clicked="OnOption3Clicked"  Margin="10"/>
            </StackLayout>
        </ContentPage>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage x:Name="NavPage" Title="Inicio" />
    </FlyoutPage.Detail>
</FlyoutPage>
